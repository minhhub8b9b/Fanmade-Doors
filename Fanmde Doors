require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Fanmade Doors", true)
wait(2)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("By Trần Bình Minh", true)

-- Smiley
coroutine.wrap(function()
    while true do
        wait(160)
        game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
        wait(0)
loadstring(game:HttpGet("https://raw.githubusercontent.com/minhhub8b9b/Fanmade-Doors/refs/heads/main/Smiley"))()

end

end)()

